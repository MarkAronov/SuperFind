{
	"name": "skillvector",
	"scripts": {
		"start": "SKIP_STATIC_DATA=true bun run src/index.ts",
		"dev": "bun run --hot src/index.ts",
		"dev:quick": "SKIP_STATIC_DATA=true bun run --hot src/index.ts",
		"lint": "biome lint ./src",
		"format": "biome format ./src --write",
		"check": "biome check ./src",
		"check:fix": "biome check ./src --fix",
		"check:format": "biome format ./src --check",
		"check:format:fix": "biome format ./src --check --write",
		"csv:validate": "node scripts/validate-csv.js",
		"csv:normalize": "node scripts/normalize-csv.js --inplace"
	},
	"dependencies": {
		"@huggingface/inference": "^4.8.0",
		"@langchain/anthropic": "^0.3.28",
		"@langchain/community": "^0.3.56",
		"@langchain/core": "^0.3.72",
		"@langchain/google-genai": "^0.2.18",
		"@langchain/ollama": "^0.2.4",
		"@langchain/openai": "^0.6.9",
		"@langchain/qdrant": "^0.1.3",
		"@qdrant/js-client-rest": "^1.15.1",
		"hono": "^4.9.0",
		"hono-rate-limiter": "^0.4.2",
		"langchain": "^0.3.31",
		"papaparse": "^5.5.3",
		"zod": "^4.1.4"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.4",
		"@types/bun": "latest",
		"@types/papaparse": "^5.3.16",
		"csv-parse": "^6.1.0",
		"csv-stringify": "^6.6.0"
	}
}
