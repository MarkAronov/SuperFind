{
	"buildCommand": "bun run build",
	"outputDirectory": "dist",
	"installCommand": "bun install",
	"framework": "vite",
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	],
	"headers": [
		{
			"source": "/assets/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	],
	"env": {
		"VITE_API_URL": "@vite_api_url"
	}
}
