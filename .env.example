# SuperFind - Environment Configuration

# =============================================================================
# API KEYS - Choose your preferred AI providers
# =============================================================================

# OpenAI (Recommended for best quality)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude (Alternative to OpenAI)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Gemini (Google's AI models)
GOOGLE_API_KEY=your_google_api_key_here

# Hugging Face (Open source models)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# =============================================================================
# AI MODEL CONFIGURATION - Customize which specific models to use
# =============================================================================

# OpenAI Models
OPENAI_MODEL=gpt-4o-mini
# Options: gpt-4o, gpt-4o-mini, gpt-4, gpt-3.5-turbo

# Anthropic Models  
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022
# Options: claude-3-5-sonnet-20241022, claude-3-opus-20240229, claude-3-sonnet-20240229

# Google Gemini Models
GEMINI_MODEL=gemini-1.5-pro
# Options: gemini-1.5-pro, gemini-1.5-flash, gemini-pro

# Hugging Face Models
HUGGINGFACE_MODEL=mistralai/Mistral-7B-Instruct-v0.1
# Options: Any Hugging Face model ID

# Ollama (Local AI models)
OLLAMA_MODEL=llama3.2
OLLAMA_BASE_URL=http://localhost:11434
# Options: llama3.2, codellama, mistral, etc.

# =============================================================================
# EMBEDDING MODEL CONFIGURATION - For vector search and similarity
# =============================================================================

# OpenAI Embeddings
OPENAI_SMALL_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_LARGE_EMBEDDING_MODEL=text-embedding-3-large

# Ollama Embeddings (Local, free)
OLLAMA_EMBEDDING_MODEL=mxbai-embed-large
OLLAMA_MXBAI_EMBEDDING_MODEL=mxbai-embed-large

# Hugging Face Embeddings
HUGGINGFACE_EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
HUGGINGFACE_BGE_EMBEDDING_MODEL=BAAI/bge-large-en-v1.5

# Google Embeddings
GOOGLE_EMBEDDING_MODEL=text-embedding-004

# =============================================================================
# VECTOR DATABASE CONFIGURATION - Qdrant for storing embeddings
# =============================================================================

QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_PROTOCOL=http
QDRANT_API_KEY=your_qdrant_api_key_here

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================

PORT=3000
NODE_ENV=development

# Qdrant Batch Processing
QDRANT_MAX_BATCH_SIZE=100
QDRANT_RETRY_ATTEMPTS=3
QDRANT_RETRY_DELAY=1000

# Embedding Configuration
EMBEDDING_PROVIDER=openai
EMBEDDING_MODEL=text-embedding-ada-002
EMBEDDING_DIMENSION=1536
EMBEDDING_BASE_URL=
